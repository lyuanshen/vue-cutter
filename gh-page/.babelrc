{
  "presets": [
    ["env", { "modules": false }],
    ["es2015", { "modules": false }],
    "stage-3"
  ],
  "plugins": [
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ],
    ["import", {
      "libraryName": "view-design",
      "libraryDirectory": "src/components"
    }],
    "syntax-dynamic-import",
    "transform-runtime"
  ]
}
